{
    "family": "euglena",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [
                {
                    "name": "IS_PERSISTENT",
                    "value": "TRUE"
                },
                {
                    "name": "PERSIST_DIRECTORY",
                    "value": "/chroma-data"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/api/v1/heartbeat || exit 1"
                ],
                "interval": 60,
                "retries": 3,
                "startPeriod": 180,
                "timeout": 10
            },
            "image": "chromadb/chroma:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "chroma"
                }
            },
            "mountPoints": [
                {
                    "sourceVolume": "chroma-data",
                    "containerPath": "/chroma-data",
                    "readOnly": false
                }
            ],
            "name": "chroma",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "command": [
                "redis-server"
            ],
            "cpu": 0,
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "redis-cli ping | grep -q PONG || exit 1"
                ],
                "interval": 60,
                "retries": 3,
                "startPeriod": 120,
                "timeout": 10
            },
            "image": "redis:7-alpine",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "redis"
                }
            },
            "mountPoints": [],
            "name": "redis",
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "command": [
                "sh",
                "-c",
                "#!/bin/sh\n# Don't use set -e here - permission changes may fail on EFS and that's OK\nmkdir -p /var/lib/rabbitmq\n# Fix permissions on EFS mount (may fail if not EFS or already correct, that's OK)\nchown -R rabbitmq:rabbitmq /var/lib/rabbitmq 2>/dev/null || true\nchmod -R 755 /var/lib/rabbitmq 2>/dev/null || true\n# Set up Erlang cookie if provided\nif [ -n \"$RABBITMQ_ERLANG_COOKIE\" ]; then\n    mkdir -p /var/lib/rabbitmq\n    echo \"$RABBITMQ_ERLANG_COOKIE\" > /var/lib/rabbitmq/.erlang.cookie\n    chmod 600 /var/lib/rabbitmq/.erlang.cookie\n    chown rabbitmq:rabbitmq /var/lib/rabbitmq/.erlang.cookie || true\nfi\n# Start RabbitMQ (use exec to replace shell process)\nexec /usr/local/bin/docker-entrypoint.sh rabbitmq-server"
            ],
            "cpu": 0,
            "environment": [
                {
                    "name": "RABBITMQ_VM_MEMORY_HIGH_WATERMARK",
                    "value": "0.6"
                },
                {
                    "name": "RABBITMQ_DISK_FREE_LIMIT",
                    "value": "2GB"
                },
                {
                    "name": "RABBITMQ_DISK_FREE_ABSOLUTE",
                    "value": "2147483648"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "rabbitmq-diagnostics ping || exit 1"
                ],
                "interval": 60,
                "retries": 5,
                "startPeriod": 240,
                "timeout": 15
            },
            "image": "rabbitmq:3-management",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "rabbitmq"
                }
            },
            "mountPoints": [
                {
                    "sourceVolume": "rabbitmq-data",
                    "containerPath": "/var/lib/rabbitmq",
                    "readOnly": false
                }
            ],
            "name": "rabbitmq",
            "portMappings": [
                {
                    "containerPort": 5672,
                    "hostPort": 5672,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 15672,
                    "hostPort": 15672,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "RABBITMQ_ERLANG_COOKIE",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:RABBITMQ_ERLANG_COOKIE::"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "START",
                    "containerName": "chroma"
                },
                {
                    "condition": "START",
                    "containerName": "redis"
                },
                {
                    "condition": "START",
                    "containerName": "rabbitmq"
                }
            ],
            "environment": [
                {
                    "name": "AGENT_INPUT_QUEUE",
                    "value": "agent.mandates"
                },
                {
                    "name": "AGENT_STATUS_QUEUE",
                    "value": "agent.status"
                },
                {
                    "name": "AGENT_STATUS_TIME",
                    "value": "10"
                },
                {
                    "name": "CHROMA_URL",
                    "value": "http://localhost:8000"
                },
                {
                    "name": "COLD_START_SECONDS",
                    "value": "120"
                },
                {
                    "name": "CORS_ALLOWED_ORIGINS",
                    "value": "https://web-rag-nine.vercel.app,http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000,http://127.0.0.1:5173"
                },
                {
                    "name": "DAILY_TICK_LIMIT",
                    "value": "256"
                },
                {
                    "name": "DEFAULT_DELAY",
                    "value": "2"
                },
                {
                    "name": "DEFAULT_TIMEOUT",
                    "value": "10"
                },
                {
                    "name": "GATEWAY_URL",
                    "value": "http://localhost:8080"
                },
                {
                    "name": "JITTER_SECONDS",
                    "value": "0.5"
                },
                {
                    "name": "LITE",
                    "value": "true"
                },
                {
                    "name": "MODEL_API_URL",
                    "value": "https://api.openai.com/v1/"
                },
                {
                    "name": "MODEL_NAME",
                    "value": "gpt-4o"
                },
                {
                    "name": "RABBITMQ_URL",
                    "value": "amqp://guest:guest@localhost:5672/"
                },
                {
                    "name": "REDIS_URL",
                    "value": "redis://localhost:6379/0"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8081/health || exit 1"
                ],
                "interval": 60,
                "retries": 3,
                "startPeriod": 180,
                "timeout": 10
            },
            "image": "ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/euglena/agent:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "agent"
                }
            },
            "mountPoints": [],
            "name": "agent",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "hostPort": 8081,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:OPENAI_API_KEY::"
                },
                {
                    "name": "RABBITMQ_ERLANG_COOKIE",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:RABBITMQ_ERLANG_COOKIE::"
                },
                {
                    "name": "SEARCH_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SEARCH_API_KEY::"
                },
                {
                    "name": "SUPABASE_ALLOW_UNCONFIRMED",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_ALLOW_UNCONFIRMED::"
                },
                {
                    "name": "SUPABASE_ANON_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_ANON_KEY::"
                },
                {
                    "name": "SUPABASE_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_API_KEY::"
                },
                {
                    "name": "SUPABASE_JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_JWT_SECRET::"
                },
                {
                    "name": "SUPABASE_URL",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_URL::"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "START",
                    "containerName": "chroma"
                },
                {
                    "condition": "START",
                    "containerName": "redis"
                },
                {
                    "condition": "START",
                    "containerName": "rabbitmq"
                }
            ],
            "environment": [
                {
                    "name": "AGENT_INPUT_QUEUE",
