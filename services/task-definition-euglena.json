                    "valueFrom": "arn:aws:secretsmanager:us-east-2:848960888155:secret:euglena-secrets-zFuHS6:SEARCH_API_KEY::"
                },
                {
                    "name": "SUPABASE_ALLOW_UNCONFIRMED",
                    "valueFrom": "arn:aws:secretsmanager:us-east-2:848960888155:secret:euglena-secrets-zFuHS6:SUPABASE_ALLOW_UNCONFIRMED::"
                },
                {
                    "name": "SUPABASE_ANON_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-2:848960888155:secret:euglena-secrets-zFuHS6:SUPABASE_ANON_KEY::"
                },
                {
                    "name": "SUPABASE_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-2:848960888155:secret:euglena-secrets-zFuHS6:SUPABASE_API_KEY::"
                },
                {
                    "name": "SUPABASE_JWT_SECRET",
