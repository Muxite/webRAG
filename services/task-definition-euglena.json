                },
                {
                    "name": "GATEWAY_URL",
                    "value": "http://localhost:8080"
                },
                {
                    "name": "JITTER_SECONDS",
                    "value": "0.5"
                },
                {
                    "name": "LITE",
                    "value": "true"
                },
                {
                    "name": "MODEL_API_URL",
                    "value": "https://api.openai.com/v1/"
                },
                {
                    "name": "MODEL_NAME",
                    "value": "gpt-4o"
                },
                {
                    "name": "RABBITMQ_URL",
                    "value": "amqp://guest:guest@localhost:5672/"
                },
                {
                    "name": "REDIS_URL",
                    "value": "redis://localhost:6379/0"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/health || exit 1"
                ],
                "interval": 60,
                "retries": 5,
                "startPeriod": 300,
                "timeout": 10
            },
            "image": "848960888155.dkr.ecr.us-east-2.amazonaws.com/euglena/gateway:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "gateway"
