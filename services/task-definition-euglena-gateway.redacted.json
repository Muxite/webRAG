{
    "family": "euglena-gateway",
    "containerDefinitions": [
        {
            "cpu": 0,
            "environment": [
                {
                    "name": "IS_PERSISTENT",
                    "value": "TRUE"
                },
                {
                    "name": "PERSIST_DIRECTORY",
                    "value": "/chroma-data"
                },
                {
                    "name": "SENTENCE_TRANSFORMERS_HOME",
                    "value": "/chroma-data/.cache"
                },
                {
                    "name": "TRANSFORMERS_CACHE",
                    "value": "/chroma-data/.cache/huggingface"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8000/api/v1/heartbeat || (echo 'Chroma health check failed' && exit 1)"
                ],
                "interval": 60,
                "retries": 6,
                "startPeriod": 300,
                "timeout": 20
            },
            "image": "chromadb/chroma:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "chroma"
                }
            },
            "mountPoints": [
                {
                    "sourceVolume": "chroma-data",
                    "containerPath": "/chroma-data",
                    "readOnly": false
                }
            ],
            "name": "chroma",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "command": [
                "redis-server"
            ],
            "cpu": 0,
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "redis-cli ping | grep PONG || exit 1"
                ],
                "interval": 90,
                "retries": 6,
                "startPeriod": 120,
                "timeout": 15
            },
            "image": "redis:7-alpine",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "redis"
                }
            },
            "mountPoints": [],
            "name": "redis",
            "portMappings": [
                {
                    "containerPort": 6379,
                    "hostPort": 6379,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "command": [
                "sh",
                "-c",
                "if [ -n \"$RABBITMQ_ERLANG_COOKIE\" ]; then mkdir -p /var/lib/rabbitmq && echo \"$RABBITMQ_ERLANG_COOKIE\" > /var/lib/rabbitmq/.erlang.cookie && chmod 600 /var/lib/rabbitmq/.erlang.cookie && chown rabbitmq:rabbitmq /var/lib/rabbitmq/.erlang.cookie; fi && exec /usr/local/bin/docker-entrypoint.sh rabbitmq-server"
            ],
            "cpu": 0,
            "environment": [],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "rabbitmq-diagnostics ping || exit 1"
                ],
                "interval": 90,
                "retries": 6,
                "startPeriod": 300,
                "timeout": 15
            },
            "image": "rabbitmq:3-management",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "rabbitmq"
                }
            },
            "mountPoints": [
                {
                    "sourceVolume": "rabbitmq-data",
                    "containerPath": "/var/lib/rabbitmq",
                    "readOnly": false
                }
            ],
            "name": "rabbitmq",
            "portMappings": [
                {
                    "containerPort": 5672,
                    "hostPort": 5672,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 15672,
                    "hostPort": 15672,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "RABBITMQ_ERLANG_COOKIE",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:RABBITMQ_ERLANG_COOKIE::"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "START",
                    "containerName": "redis"
                },
                {
                    "condition": "START",
                    "containerName": "rabbitmq"
                }
            ],
            "environment": [
                {
                    "name": "AGENT_INPUT_QUEUE",
                    "value": "agent.mandates"
                },
                {
                    "name": "AGENT_STATUS_QUEUE",
                    "value": "agent.status"
                },
                {
                    "name": "AGENT_STATUS_TIME",
                    "value": "10"
                },
                {
                    "name": "CHROMA_URL",
                    "value": "http://localhost:8000"
                },
                {
                    "name": "COLD_START_SECONDS",
                    "value": "120"
                },
                {
                    "name": "CORS_ALLOWED_ORIGINS",
                    "value": "https://web-rag-nine.vercel.app,http://localhost:3000,http://localhost:5173,http://127.0.0.1:3000,http://127.0.0.1:5173"
                },
                {
                    "name": "DAILY_TICK_LIMIT",
                    "value": "256"
                },
                {
                    "name": "DEFAULT_DELAY",
                    "value": "2"
                },
                {
                    "name": "DEFAULT_TIMEOUT",
                    "value": "10"
                },
                {
                    "name": "GATEWAY_URL",
                    "value": "http://localhost:8080"
                },
                {
                    "name": "JITTER_SECONDS",
                    "value": "0.5"
                },
                {
                    "name": "LITE",
                    "value": "true"
                },
                {
                    "name": "MODEL_API_URL",
                    "value": "https://api.openai.com/v1/"
                },
                {
                    "name": "MODEL_NAME",
                    "value": "gpt-4o"
                },
                {
                    "name": "RABBITMQ_URL",
                    "value": "amqp://guest:guest@localhost:5672/"
                },
                {
                    "name": "REDIS_URL",
                    "value": "redis://localhost:6379/0"
                }
            ],
            "essential": true,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8080/health || exit 1"
                ],
                "interval": 90,
                "retries": 6,
                "startPeriod": 300,
                "timeout": 15
            },
            "image": "ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/euglena/gateway:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "gateway"
                }
            },
            "mountPoints": [],
            "name": "gateway",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "secrets": [
                {
                    "name": "OPENAI_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:OPENAI_API_KEY::"
                },
                {
                    "name": "RABBITMQ_ERLANG_COOKIE",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:RABBITMQ_ERLANG_COOKIE::"
                },
                {
                    "name": "SEARCH_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SEARCH_API_KEY::"
                },
                {
                    "name": "SUPABASE_ALLOW_UNCONFIRMED",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_ALLOW_UNCONFIRMED::"
                },
                {
                    "name": "SUPABASE_ANON_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_ANON_KEY::"
                },
                {
                    "name": "SUPABASE_API_KEY",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_API_KEY::"
                },
                {
                    "name": "SUPABASE_JWT_SECRET",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_JWT_SECRET::"
                },
                {
                    "name": "SUPABASE_URL",
                    "valueFrom": "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:SECRET_NAME-SUFFIX:SUPABASE_URL::"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        },
        {
            "cpu": 0,
            "dependsOn": [
                {
                    "condition": "START",
                    "containerName": "rabbitmq"
                }
            ],
            "environment": [
                {
                    "name": "RABBITMQ_URL",
                    "value": "amqp://guest:guest@localhost:5672/"
                },
                {
                    "name": "PUBLISH_QUEUE_DEPTH_METRICS",
                    "value": "true"
                },
                {
                    "name": "QUEUE_DEPTH_METRICS_INTERVAL",
                    "value": "5"
                },
                {
                    "name": "CLOUDWATCH_NAMESPACE",
                    "value": "Euglena/RabbitMQ"
                },
                {
                    "name": "QUEUE_NAME",
                    "value": "agent.mandates"
                }
            ],
            "essential": false,
            "healthCheck": {
                "command": [
                    "CMD-SHELL",
                    "curl -f http://localhost:8082/health || exit 1"
                ],
                "interval": 30,
                "retries": 3,
                "startPeriod": 60,
                "timeout": 10
            },
            "image": "ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/euglena/metrics:latest",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/euglena",
                    "awslogs-region": "REGION",
                    "awslogs-stream-prefix": "metrics"
                }
            },
            "mountPoints": [],
            "name": "metrics",
            "portMappings": [
                {
                    "containerPort": 8082,
                    "hostPort": 8082,
                    "protocol": "tcp"
                }
            ],
            "systemControls": [],
            "volumesFrom": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::ACCOUNT_ID:role/ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::ACCOUNT_ID:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "chroma-data",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0ec151e2adb754fc8",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "iam": "ENABLED"
                }
            }
        },
        {
            "name": "rabbitmq-data",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-0ec151e2adb754fc8",
                "transitEncryption": "ENABLED",
                "authorizationConfig": {
                    "iam": "ENABLED"
                }
            }
        }
    ],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}